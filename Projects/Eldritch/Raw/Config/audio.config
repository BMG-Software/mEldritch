Stream				= false
3DSound				= false
Volume				= 1.0

[SoundArchetypePlayerSound]
Category			= "SFX"
Stream				= false
3DSound				= false
Volume				= 1.0

[SoundArchetypeMusic]
Category			= "Music"
Stream				= true
3DSound				= false
Volume				= 1.0
Looping				= true

[SoundArchetypeUI]
