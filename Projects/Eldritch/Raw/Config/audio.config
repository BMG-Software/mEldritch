Room				= -1200.0
RoomHF				= -600.0
DecayTime			= 1.5
DecayHFRatio		= 0.25
ReflectionsLevel	= -1500.0
ReflectionsDelay	= 0.02
ReverbLevel			= 0.0
ReverbDelay			= 0.02
Diffusion			= 100.0
Density				= 100.0
HFReference			= 5000.0
RoomLF				= -1800.0
LFReference			= 250.0

