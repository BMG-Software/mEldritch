[SoundArchetypeLoop]
Category			= "SFX"
Stream				= false
3DSound				= true
ShouldCalcOcclusion	= true
Looping				= true
Volume				= 1.0
FalloffRadius		= 5.0
PanBiasNear			= 0.5
PanBiasFar			= 1.0
PanPower			= 0.5
MinimumAttenuation	= 0.05
RearAttenuation		= 0.5	# NOTE: 1.0 means full rear attenuation

[SoundArchetypeOneShot]
Category			= "SFX"
Stream				= false
3DSound				= true
ShouldCalcOcclusion	= true
Volume				= 1.0
FalloffRadius		= 5.0
PanBiasNear			= 0.5
PanBiasFar			= 1.0
PanPower			= 0.5
MinimumAttenuation	= 0.05
RearAttenuation		= 0.5	# NOTE: 1.0 means full rear attenuation

[SoundArchetypeBark]
Category			= "VO"
Stream				= true
3DSound				= true
ShouldCalcOcclusion	= true
Volume				= 1.0
FalloffRadius		= 5.0
PanBiasNear			= 0.5
PanBiasFar			= 1.0
PanPower			= 0.5
MinimumAttenuation	= 0.05
RearAttenuation		= 0.5	# NOTE: 1.0 means full rear attenuation

[SoundArchetypePlayerBark]
