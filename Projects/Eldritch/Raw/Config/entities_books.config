[Guide]
EldTransform	= "."
EldMesh			= "GuideMesh"
EldFrobbable	= "GuideFrobbable"

	[GuideMesh]
	Mesh	= "Meshes/book-static.cms"

	[GuideFrobbable]
	UseMeshExtents	= true
	ExtentsFatten	= 0.05
	FriendlyName	= "Book"
	FrobVerb		= "Read"

[GuideArtifact]
Extends		= "Guide"
Reactions	= "GuideArtifactReactions"

	[GuideArtifactReactions]
	NumReactions	= &
	@ Reaction
	@@&				= "GuideArtifactOnFrobbedReaction"

		[GuideArtifactOnFrobbedReaction]
		Rule		= "OnFrobbedRule"
		NumActions	= &
		@ Action
		@@&			= "ShowArtifactBook"

