		[GuideJumpOnFrobbedReaction]
		Rule		= "OnFrobbedRule"
		NumActions	= &
		@ Action
		@@&			= "ShowJumpBook"

			[ShowJumpBook]
			ActionType	= "EldShowBook"
			BookString	= "Book_Jump"
			IsDynamic	= true

